# Postgres Database 
DATABASE_URL=postgresql://username:password@localhost:5432/digital-menu?sslmode=disable

# FOR DOCKER COMPOSE FILE
POSTGRES_DB=digital-menu
POSTGRES_USER=<your_postgres_user>
POSTGRES_PASSWORD=<your_postgres_password>

# MinIO Credentials, FOR ENV INSIDE DOCKER COMPOSE FILE 
MINIO_ROOT_USER=<your_minio_user>
MINIO_ROOT_PASSWORD=<your_minio_password>

# MinIO Integration With Frontend (Next.js)
# use host.docker.internal or kubernetes.docker.internal to connect from host machine
S3_ENDPOINT=kubernetes.docker.internal 
S3_PORT=9000
# S3_ACCESS_KEY and SECRET use same as MINIO_ROOT_USER and MINIO_ROOT_PASSWORD for Self-Hosted Way
S3_ACCESS_KEY=<your_minio_user>
S3_SECRET_KEY=<your_minio_password>
S3_USE_SSL=false
NEXT_PUBLIC_S3_BUCKET_NAME=digital-menu


NEXT_TURBOPACK_TRACING=1
NEXT_PUBLIC_SERVER_URL=http://localhost:3000
CORS_ORIGIN=http://localhost:3000

BETTER_AUTH_SECRET=<your_better_auth_secret>
APP_NAME=Digital_Menu

# GitHub OAuth
GITHUB_CLIENT_ID=<your_github_client_id>
GITHUB_CLIENT_SECRET=<your_github_client_secret>


# Google OAuth
GOOGLE_CLIENT_ID=<your_google_client_id>
GOOGLE_CLIENT_SECRET=<your_google_client_secret>

# PostHog Analytics
NEXT_PUBLIC_POSTHOG_KEY=<your_posthog_key>
NEXT_PUBLIC_POSTHOG_HOST=<your_posthog_host>
