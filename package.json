{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port=3000",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "trace": "next internal turbo-trace-server .next/trace-turbopack",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev --name init",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "docker:up": "docker compose --env-file .env up -d",
    "docker:down": "docker compose down",
    "auth:generate": "bunx @better-auth/cli generate --output ./src/prisma/schema/auth.prisma --yes",
    "db:init": "bun run db:reset && bun run db:generate && bun run auth:generate && bun run db:migrate"
  },
  "dependencies": {
    "@better-auth/cli": "^1.3.7",
    "@orpc/client": "^1.8.5",
    "@orpc/server": "^1.8.5",
    "@orpc/tanstack-query": "^1.5.0",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-form": "^1.12.3",
    "@tanstack/react-query": "^5.80.5",
    "@vavt/v3-extension": "^3.0.0",
    "better-auth": "^1.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "ioredis": "^5.8.1",
    "lucide-react": "^0.487.0",
    "md-editor-rt": "^5.8.4",
    "minio": "^8.0.6",
    "next": "^15.5.0",
    "next-intl": "^4.3.5",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.277.0",
    "posthog-node": "^5.9.1",
    "radix-ui": "^1.4.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "zod": "^4.0.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query-devtools": "^5.80.5",
    "@types/bun": "^1.2.6",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15.5.0",
    "prisma": "^6.14.0",
    "tailwindcss": "^4.1.10",
    "typescript": "^5"
  },
  "packageManager": "bun@1.2.16"
}
